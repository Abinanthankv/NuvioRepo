    <!-- Premium Popup Modal -->
    <div id="premiumPopup" class="fixed inset-0 z-50 hidden flex items-center justify-center px-4">
        <div class="absolute inset-0 bg-black/80 backdrop-blur-sm" onclick="PremiumPopup.close()"></div>
        <div class="relative bg-gray-900 border border-red-500/30 rounded-2xl p-6 max-w-md w-full shadow-2xl transform transition-all scale-100">
            <button onclick="PremiumPopup.close()" class="absolute top-4 right-4 text-gray-400 hover:text-white">
                <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"></path></svg>
            </button>

            <div class="text-center">
                <div class="w-16 h-16 bg-red-600/20 rounded-full flex items-center justify-center mx-auto mb-4 border border-red-500/50">
