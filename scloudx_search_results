    <script>
        // Premium Popup Logic
        const PremiumPopup = {
            init() {
                this.checkAndShow();
            },

            checkAndShow() {
                const now = Date.now();
                const nextShowTime = parseInt(localStorage.getItem('scloud_next_popup') || '0');

                if (now >= nextShowTime) {
                    this.show();
                }
            },

            show() {
                const popup = document.getElementById('premiumPopup');
                if (popup) {
                    popup.classList.remove('hidden');
                    document.body.style.overflow = 'hidden';
                }
            },

            close() {
                const popup = document.getElementById('premiumPopup');
                if (popup) {
                    popup.classList.add('hidden');
                    document.body.style.overflow = '';
                    this.scheduleNext();
                }
            },

            scheduleNext() {
                const now = Date.now();
                const consecutiveCloses = parseInt(localStorage.getItem('scloud_popup_closes') || '0');

                let nextTime;
                let nextCloses;

                if (consecutiveCloses === 0) {
                    // First close: show again in 1 hour
                    nextTime = now + (60 * 60 * 1000); // 1 hour
                    nextCloses = 1;
                } else {
                    // Second close: show again in 3 days
                    nextTime = now + (3 * 24 * 60 * 60 * 1000); // 3 days
                    nextCloses = 0; // Reset cycle
                }

                localStorage.setItem('scloud_next_popup', nextTime.toString());
                localStorage.setItem('scloud_popup_closes', nextCloses.toString());
            }
        };

        // Mobile performance optimization
        const isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);

        // Initialize animations on page load
        document.addEventListener('DOMContentLoaded', function() {
            // Initialize Premium Popup
            PremiumPopup.init();

            // Optimize scrolling behavior
            if (isMobile) {
                // Use native scrolling on mobile for better performance
                document.documentElement.style.scrollBehavior = 'auto';
                // Enable touch scrolling optimization
                document.body.style.webkitOverflowScrolling = 'touch';
            } else {
                // Smooth scrolling on desktop
                document.documentElement.style.scrollBehavior = 'smooth';
            }

            // Add passive event listeners for better performance on mobile
            if (isMobile) {
                document.addEventListener('touchstart', () => {}, { passive: true });
                document.addEventListener('touchmove', () => {}, { passive: true });
            }
        });
    </script>

    <script>
        /**
         * SearchApp - Encapsulates all search functionality
         * Provides a clean, namespaced environment for search operations
         */
        const SearchApp = {
            // State
            isLoading: false,
            isMobile: false,

            // Initialization
            init() {
                this.isMobile = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);
                this.bindEvents();
                this.setupUI();
            },

            // Bind all event listeners
            bindEvents() {
                // Search Form
                const form = document.getElementById('searchForm');
                if (form) {
                    form.addEventListener('submit', (e) => this.processSearch(e));
                }

                // Search Button Animation
                const searchButton = document.getElementById('searchButton');
                if (searchButton && !this.isMobile) {
                    searchButton.addEventListener('mousedown', () => {
                        if (!this.isLoading) searchButton.style.transform = 'scale(0.98)';
                    });
                    searchButton.addEventListener('mouseup', () => {
                        if (!this.isLoading) searchButton.style.transform = 'scale(1)';
                    });
                }

                // Trending Tags (Delegation)
                document.querySelectorAll('.js-trend-search').forEach(btn => {
                    btn.addEventListener('click', (e) => {
                        const term = e.currentTarget.dataset.term;
                        if (term) this.searchWithTrend(term);
                    });
                });

                // Sort Buttons
                document.querySelectorAll('.js-sort-btn').forEach(btn => {
                    btn.addEventListener('click', (e) => {
                        const direction = e.currentTarget.dataset.sort;
                        if (direction) this.sortResultsBySize(direction);
                    });
                });

                // Select All Button
                const selectAllBtn = document.getElementById('selectAllButton');
                if (selectAllBtn) {
                    selectAllBtn.addEventListener('click', () => this.selectAllCheckboxes());
                }

                // Copy Button
                const copyBtn = document.getElementById('copyButton');
                if (copyBtn) {
                    copyBtn.addEventListener('click', () => this.copySelectedLinks());
                }

                // Stop Propagation for Checkboxes
                document.querySelectorAll('.js-stop-prop').forEach(el => {
                    el.addEventListener('click', (e) => e.stopPropagation());
                });

                // Input Focus on iOS
                if (/iPhone|iPad|iPod/i.test(navigator.userAgent)) {
                    const searchInput = document.getElementById('searchInput');
                    if (searchInput) {
                        searchInput.addEventListener('focus', function() {
                            this.style.fontSize = '16px';
                        });
                    }
                }

                // Handle Back/Forward Cache
                window.addEventListener('pageshow', () => {
                    this.isLoading = false;
                    const overlay = document.getElementById('loadingOverlay');
                    if (overlay) overlay.classList.remove('active');
                });
            },

            // UI Setup
            setupUI() {
                // Reset loading state
                this.isLoading = false;
                const overlay = document.getElementById('loadingOverlay');
                if (overlay) overlay.classList.remove('active');

                // Prevent form resubmission
                if (window.history.replaceState) {
                    window.history.replaceState(null, null, window.location.href);
                }
            },

            // Core Search Logic
            processSearch(e) {
                e.preventDefault();

                if (this.isLoading) return;

                const searchInput = document.getElementById('searchInput');
                if (!searchInput) return;

                const term = searchInput.value.trim();

                if (term) {
                    this.submitSearch(term);
                }
            },

            searchWithTrend(term) {
                if (this.isLoading) return;
                this.submitSearch(term);
            },

            submitSearch(term) {
                // Save to history
                this.saveToHistory(term);

                this.isLoading = true;
                document.getElementById('loadingOverlay').classList.add('active');

                const form = document.createElement('form');
                form.method = 'POST';
                form.action = '/get-search-token';

                const input = document.createElement('input');
                input.type = 'hidden';
                input.name = 'search_query';
                input.value = term;

                form.appendChild(input);
                document.body.appendChild(form);
                form.submit();
            },

            saveToHistory(term) {
                const historyItem = {
                    title: term,
                    type: 'search',
                    timestamp: Date.now()
                };

                let history = JSON.parse(localStorage.getItem('scloud_history') || '[]');

                // Remove duplicate if exists
                history = history.filter(item => item.title.toLowerCase() !== term.toLowerCase());

                // Add to beginning
                history.unshift(historyItem);

                // Limit to 50 items
                if (history.length > 50) history.pop();

                localStorage.setItem('scloud_history', JSON.stringify(history));
            },

            // Utility Functions
            copySelectedLinks() {
                const checkboxes = document.querySelectorAll('input.copy-checkbox:checked');
                const links = Array.from(checkboxes).map(cb => cb.getAttribute('data-url'));

                if (links.length === 0) {
                    this.showNotification('No links selected', 'warning');
                    return;
                }

                const textToCopy = links.join('\n');
                navigator.clipboard.writeText(textToCopy)
                    .then(() => this.showNotification(`Copied ${links.length} links to clipboard`, 'success'))
                    .catch(() => this.showNotification('Failed to copy links', 'error'));
            },

            selectAllCheckboxes() {
                const checkboxes = document.querySelectorAll('input.copy-checkbox');
                const anyUnchecked = [...checkboxes].some(checkbox => !checkbox.checked);

                checkboxes.forEach((checkbox) => {
                    checkbox.checked = anyUnchecked;
                });

                const selectAllButton = document.getElementById('selectAllButton');
                if (selectAllButton) {
                    selectAllButton.textContent = anyUnchecked ? 'Deselect All' : 'Select All';
                }
            },

            convertSizeToBytes(sizeStr) {
                const units = {
                    'B': 1,
                    'KB': 1024,
                    'MB': 1024 ** 2,
                    'GB': 1024 ** 3,
                    'TB': 1024 ** 4,
                    'KIB': 1024,
                    'MIB': 1024 ** 2,
                    'GIB': 1024 ** 3,
                    'TIB': 1024 ** 4
                };

                sizeStr = sizeStr.replace(/\s+/g, '').toUpperCase();
                const match = sizeStr.match(/^([\d.]+)([A-Z]+)$/);

                if (match) {
                    const number = parseFloat(match[1]);
                    const unit = match[2];

                    if (units[unit]) {
                        return number * units[unit];
                    }
                }
                return 0;
            },

            sortResultsBySize(direction = 'asc') {
                const resultsContainer = document.getElementById('resultsContainer');
                if (!resultsContainer) return;

                const resultItems = Array.from(document.querySelectorAll('.result-item'));

                resultItems.sort((a, b) => {
                    const aSize = this.convertSizeToBytes(a.getAttribute('data-size') || '0');
                    const bSize = this.convertSizeToBytes(b.getAttribute('data-size') || '0');

                    return direction === 'asc' ? aSize - bSize : bSize - aSize;
                });

                resultsContainer.innerHTML = '';
                resultItems.forEach(item => {
                    resultsContainer.appendChild(item);
                });

                this.showNotification(`Sorted by size (${direction === 'asc' ? 'smallest first' : 'largest first'})`, 'info');
            },

            showNotification(message, type = 'info') {
                const notification = document.createElement('div');
                notification.className = `fixed top-4 right-4 px-4 py-3 rounded-lg font-medium transition-all transform translate-x-full z-50 max-w-sm`;

                const colors = {
                    success: 'bg-green-600 text-white border border-green-500 shadow-lg',
                    error: 'bg-red-600 text-white border border-red-500 shadow-lg',
                    warning: 'bg-yellow-600 text-white border border-yellow-500 shadow-lg',
                    info: 'bg-blue-600 text-white border border-blue-500 shadow-lg'
                };

                notification.className += ' ' + colors[type];
                notification.textContent = message;
                notification.style.fontSize = '0.875rem';
                notification.style.backgroundColor = type === 'success' ? '#059669' :
                                                    type === 'error' ? '#dc2626' :
                                                    type === 'warning' ? '#d97706' : '#2563eb';
                notification.style.opacity = '1';
                notification.style.backdropFilter = 'none';

                document.body.appendChild(notification);

                setTimeout(() => {
                    notification.style.transform = 'translateX(0)';
                }, 100);

                setTimeout(() => {
                    notification.style.transform = 'translateX(100%)';
                    setTimeout(() => notification.remove(), 300);
                }, 3000);
            }
        };

        // Initialize App
        document.addEventListener('DOMContentLoaded', () => SearchApp.init());
    </script>

